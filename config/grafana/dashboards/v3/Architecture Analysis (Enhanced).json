          "datasource": "Pepsico Cost Analytics",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  architecture_name as \"Architecture\",\n  SUM(app_count) as \"App Count\",\n  COALESCE(SUM(total_units), 0) as \"Total Units\",\n  ROUND(AVG(avg_nodes)::numeric, 2) as \"Avg Nodes\",\n  COALESCE(SUM(units_per_node), 0) as \"Units per Node\",\n  COALESCE(SUM(total_cost), 0) as \"Total Cost\",\n  CASE \n    WHEN SUM(app_count) > 0 \n    THEN SUM(total_cost) / SUM(app_count)\n    ELSE 0 \n  END as \"Cost per App\"\nFROM mv_architecture_metrics_90d\nWHERE ('$controller' = 'All' OR controller = '$controller')\nGROUP BY architecture_name\nHAVING SUM(app_count) > 0\n   AND (SUM(total_units) > 0 OR SUM(total_cost) > 0)\nORDER BY COALESCE(SUM(total_cost), 0) DESC",
          "refId": "A",
