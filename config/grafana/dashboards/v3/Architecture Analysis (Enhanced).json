        {
          "datasource": "Pepsico Cost Analytics",
          "format": "time_series",
          "rawSql": "WITH daily_arch_usage AS (\n  SELECT \n    usage_date as time,\n    architecture_name as metric,\n    SUM(total_units) as value\n  FROM mv_daily_usage_by_capability duv\n  JOIN applications_dim a ON duv.controller = a.appd_controller\n  LEFT JOIN architecture_dim ar ON a.architecture_id = ar.architecture_id\n  WHERE usage_date >= $__timeFrom()::date\n    AND usage_date <= $__timeTo()::date\n  GROUP BY usage_date, ar.pattern_name\n)\nSELECT time, metric, COALESCE(SUM(value), 0) as value\nFROM daily_arch_usage\nGROUP BY time, metric\nORDER BY time, metric",
          "refId": "A"
        }
      ],
      "title": "Daily Units by Architecture",
      "type": "timeseries"
