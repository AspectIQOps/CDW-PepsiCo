        {
          "datasource": "Pepsico Cost Analytics",
          "format": "table",
          "rawSql": "SELECT \n  capability_code as capability,\n  COALESCE(SUM(total_cost), 0) as total\nFROM mv_daily_usage_by_capability dv\nJOIN (\n  SELECT DISTINCT capability_code, controller, tier, SUM(total_cost) as total_cost\n  FROM (\n    SELECT \n      duv.capability_code,\n      dc.controller,\n      dc.tier,\n      dc.total_cost\n    FROM mv_daily_usage_by_capability duv\n    JOIN mv_daily_cost_by_controller dc \n      ON duv.usage_date = dc.cost_date \n      AND duv.controller = dc.controller \n      AND duv.tier = dc.tier\n    WHERE duv.usage_date >= NOW() - INTERVAL '90 days'\n      AND ('${controller}' = 'All' OR duv.controller = '${controller}')\n  ) sub\n  GROUP BY capability_code, controller, tier\n) cost ON dv.capability_code = cost.capability_code\nWHERE dv.usage_date >= NOW() - INTERVAL '90 days'\nGROUP BY dv.capability_code\nHAVING SUM(total_cost) > 0\nORDER BY total DESC",
          "refId": "A"
        }
      ],
      "title": "Cost by License Type",
      "type": "piechart"
