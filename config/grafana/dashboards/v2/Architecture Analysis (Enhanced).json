      "pluginVersion": "12.4.0-19174562009.patch3",
      "targets": [
        {
          "datasource": "Pepsico Cost Analytics",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH date_filter AS (\n  SELECT \n    NOW() - INTERVAL '90 days' as start_ts,\n    NOW() as end_ts\n),\nusage_by_app AS (\n  SELECT \n    lu.app_id,\n    SUM(lu.units_consumed) as units,\n    AVG(lu.nodes_count) as avg_nodes\n  FROM license_usage_fact lu, date_filter df\n  WHERE lu.ts >= df.start_ts AND lu.ts <= df.end_ts\n  GROUP BY lu.app_id\n),\ncost_by_app AS (\n  SELECT \n    lc.app_id,\n    SUM(lc.usd_cost) as cost\n  FROM license_cost_fact lc, date_filter df\n  WHERE lc.ts >= df.start_ts AND lc.ts <= df.end_ts\n  GROUP BY lc.app_id\n),\napps_with_arch AS (\n  SELECT \n    a.app_id,\n    COALESCE(ar.pattern_name, 'Unknown') as arch_name\n  FROM applications_dim a\n  LEFT JOIN architecture_dim ar ON a.architecture_id = ar.architecture_id\n  WHERE '$controller' = 'All' OR a.appd_controller = '$controller'\n)\nSELECT \n  awa.arch_name as \"Architecture\",\n  COUNT(DISTINCT awa.app_id) as \"App Count\",\n  COALESCE(SUM(u.units), 0) as \"Total Units\",\n  ROUND(AVG(u.avg_nodes)::numeric, 2) as \"Avg Nodes\",\n  CASE \n    WHEN AVG(u.avg_nodes) > 0 \n    THEN ROUND((SUM(u.units) / AVG(u.avg_nodes))::numeric, 2)\n    ELSE 0 \n  END as \"Units per Node\",\n  COALESCE(SUM(c.cost), 0) as \"Total Cost\",\n  CASE \n    WHEN COUNT(DISTINCT awa.app_id) > 0 \n    THEN SUM(c.cost) / COUNT(DISTINCT awa.app_id)\n    ELSE 0 \n  END as \"Cost per App\"\nFROM apps_with_arch awa\nLEFT JOIN usage_by_app u ON awa.app_id = u.app_id\nLEFT JOIN cost_by_app c ON awa.app_id = c.app_id\nGROUP BY awa.arch_name\nHAVING COUNT(DISTINCT awa.app_id) > 0\n   AND (SUM(u.units) > 0 OR SUM(c.cost) > 0)\nORDER BY COALESCE(SUM(c.cost), 0) DESC",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Architecture Efficiency Summary",
      "type": "table"
