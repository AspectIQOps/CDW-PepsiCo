      "targets": [
        {
          "datasource": "Pepsico Cost Analytics",
          "format": "table",
          "rawSql": "WITH arch_license_usage AS (\n  SELECT \n    COALESCE(ar.pattern_name, 'Unknown') as architecture,\n    c.capability_code,\n    COUNT(DISTINCT a.app_id) as app_count,\n    COALESCE(SUM(lu.units_consumed), 0) as total_units\n  FROM applications_dim a\n  LEFT JOIN architecture_dim ar ON a.architecture_id = ar.architecture_id\n  LEFT JOIN license_usage_fact lu ON a.app_id = lu.app_id AND lu.ts >= NOW() - INTERVAL '90 days'\n  LEFT JOIN capabilities_dim c ON lu.capability_id = c.capability_id\n  WHERE ('$controller' = 'All' OR a.appd_controller = '$controller')\n  GROUP BY ar.pattern_name, c.capability_code\n)\nSELECT \n  architecture as \"Architecture\",\n  MAX(CASE WHEN capability_code = 'APM' THEN app_count ELSE 0 END) as \"App Count\",\n  COALESCE(MAX(CASE WHEN capability_code = 'APM' THEN total_units / NULLIF(app_count, 0) END), 0)::integer as \"APM Units/App\",\n  COALESCE(MAX(CASE WHEN capability_code = 'MRUM' THEN total_units / NULLIF(app_count, 0) END), 0)::integer as \"MRUM Units/App\",\n  COALESCE(MAX(CASE WHEN capability_code = 'Synthetic' THEN total_units / NULLIF(app_count, 0) END), 0)::integer as \"Synthetic Units/App\",\n  COALESCE(MAX(CASE WHEN capability_code = 'DB' THEN total_units / NULLIF(app_count, 0) END), 0)::integer as \"DB Units/App\",\n  COALESCE(SUM(total_units) / NULLIF(MAX(CASE WHEN capability_code = 'APM' THEN app_count END), 0), 0)::integer as \"Total Units/App\"\nFROM arch_license_usage\nGROUP BY architecture\nHAVING MAX(CASE WHEN capability_code = 'APM' THEN app_count END) > 0\nORDER BY \"Total Units/App\" DESC",
          "refId": "A"
        }
      ],
      "title": "Average Units per Application by Architecture & License Type",
      "type": "table"
    },
