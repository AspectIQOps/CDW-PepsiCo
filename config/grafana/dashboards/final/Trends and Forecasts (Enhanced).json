      "targets": [
        {
          "datasource": "Pepsico Cost Analytics",
          "format": "time_series",
          "rawSql": "-- Show forecast data if available, otherwise show 6-month historical trend\nWITH forecast_data AS (\n  SELECT\n    DATE_TRUNC('month', month_start) as time,\n    'Projected Cost' as metric,\n    SUM(projected_cost) as value\n  FROM forecast_fact ff\n  JOIN applications_dim a ON ff.app_id = a.app_id\n  WHERE month_start BETWEEN NOW() AND NOW() + INTERVAL '18 months'\n    AND method = 'linear'\n    AND a.appd_controller IN (${controller})\n  GROUP BY DATE_TRUNC('month', month_start)\n)\nSELECT * FROM forecast_data\nUNION ALL\nSELECT\n  DATE_TRUNC('month', cost_date) as time,\n  'Historical Trend' as metric,\n  AVG(SUM(total_cost)) OVER (ORDER BY DATE_TRUNC('month', cost_date) ROWS BETWEEN 2 PRECEDING AND CURRENT ROW) as value\nFROM mv_daily_cost_by_controller\nWHERE cost_date >= NOW() - INTERVAL '6 months'\n  AND controller IN (${controller})\nGROUP BY DATE_TRUNC('month', cost_date)\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "6-Month Forecast with Confidence Bands",
      "type": "timeseries"
