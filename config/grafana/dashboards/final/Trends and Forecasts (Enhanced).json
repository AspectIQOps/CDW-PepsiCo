        {
          "datasource": "Pepsico Cost Analytics",
          "format": "time_series",
          "rawSql": "WITH monthly_costs AS (\n  SELECT\n    DATE_TRUNC('month', cost_date) as month,\n    EXTRACT(MONTH FROM cost_date) as month_num,\n    SUM(total_cost) as monthly_total\n  FROM mv_daily_cost_by_controller\n  WHERE cost_date >= NOW() - INTERVAL '24 months'\n    AND controller IN (${controller})\n  GROUP BY DATE_TRUNC('month', cost_date), EXTRACT(MONTH FROM cost_date)\n)\nSELECT\n  month as time,\n  TO_CHAR(month, 'Month') as metric,\n  AVG(monthly_total) OVER (PARTITION BY month_num) as value\nFROM monthly_costs\nGROUP BY month, month_num, monthly_total\nORDER BY month",
          "refId": "A"
        }
      ],
      "title": "Seasonal Pattern Analysis (Monthly Averages)",
      "type": "timeseries"
