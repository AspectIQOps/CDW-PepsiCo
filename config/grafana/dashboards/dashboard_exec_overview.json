        {
          "datasource": "PepsiCo AppDynamics Licensing",
          "format": "table",
          "rawSql": "WITH monthly_totals AS (\n  SELECT \n    date_trunc('month', month_start) as month,\n    SUM(usd_amount) as total\n  FROM chargeback_fact\n  WHERE month_start >= date_trunc('month', $__timeFrom()::timestamptz) - interval '1 month'\n    AND month_start <= date_trunc('month', $__timeTo()::timestamptz)\n  GROUP BY 1\n  ORDER BY 1 DESC\n  LIMIT 2\n)\nSELECT \n  CASE \n    WHEN LAG(total) OVER (ORDER BY month DESC) = 0 THEN 0\n    ELSE ((total - LAG(total) OVER (ORDER BY month DESC)) / \n          NULLIF(LAG(total) OVER (ORDER BY month DESC), 0)) * 100\n  END as mom_change\nFROM monthly_totals\nLIMIT 1",
          "refId": "A"
        }
      ],
      "title": "Monthly Cost Change (MoM)",
      "type": "stat"
