        {
          "datasource": "PepsiCo AppDynamics Licensing",
          "format": "table",
          "rawSql": "SELECT \n  a.appd_application_name as \"Application\",\n  COALESCE(s.sector_name, 'Unknown') as \"Sector\",\n  COALESCE(o.owner_name, 'Unknown') as \"Owner\",\n  COALESCE(ar.pattern_name, 'Unknown') as \"Architecture\",\n  COALESCE(SUM(lc.usd_cost), 0) as \"Total Cost\",\n  COALESCE(SUM(lu.units_consumed), 0) as \"Total Units\"\nFROM applications_dim a\nLEFT JOIN sectors_dim s ON a.sector_id = s.sector_id\nLEFT JOIN owners_dim o ON a.owner_id = o.owner_id\nLEFT JOIN architecture_dim ar ON a.architecture_id = ar.architecture_id\nLEFT JOIN license_usage_fact lu ON a.app_id = lu.app_id\n  AND lu.ts >= date_trunc('month', $__timeFrom()::timestamptz)\n  AND lu.ts < date_trunc('month', $__timeTo()::timestamptz) + interval '1 month'\nLEFT JOIN license_cost_fact lc ON a.app_id = lc.app_id\n  AND lc.ts >= date_trunc('month', $__timeFrom()::timestamptz)\n  AND lc.ts < date_trunc('month', $__timeTo()::timestamptz) + interval '1 month'\nWHERE a.appd_application_name IS NOT NULL\nGROUP BY a.appd_application_name, s.sector_name, o.owner_name, ar.pattern_name\nORDER BY \"Total Cost\" DESC\nLIMIT 10",
          "refId": "A"
        }
      ],
